mean_age <- df %>%
group_by(year, country_of_jury, whichContest) %>%
summarise(mean_age = mean(age))
res <- sex_count
res$mean_age <- mean_age$mean_age
res[res$year == 2017,]
write.csv2(res, "jury.csv", row.names = F)
